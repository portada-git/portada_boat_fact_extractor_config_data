                },
                {
                    "field":"master_name",
                    "type":"s",
                },
                {
                    "field":"cargo_list",
                    "type":"s",
                },
                {
                    "field":"ship_name",
                    "type":"s",
                },
                {
                    "field":"parsed_text",
                    "type":"s",
                }
            ]
        }
    },
