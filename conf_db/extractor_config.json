                    "type":"d",
                },
                {
                    "field":"travel_duration",
                    "type":"joined_field",
                    "fields":["travel_duration_value", "travel_duration_unit"]
                },
                {
                    "field":"travel_arrival_date",
                    "type":"d",
                },
                {
                    "field":"travel_departure_port",
